<div class="container">
	<div class="row">
		<div class="col-xs-12">
			<div class="h1">Users</div>
			<hr/>
			
			<div class="table-responsive">
				<table class="table ">
					<thead>
						<tr>
							<th>id</th>
							<th>username</th>
							<th>first name</th>
							<th>last name</th>
							<th>added</th>
						</tr>
					</thead>
					<tbody></tbody>
					{{#each user}}
					<tr>
						<td>{{id}}</td>
						<td>{{username}}</td> 
						<td>{{first_name}}</td> 
						<td>{{last_name}}</td>
						<td>{{createdAt}}</td>
					</tr>
					{{/each}}
				</table>
			</div>
			
		</div>
	</div>
</div>

